{
  "name": "gatsby-tailwind-styled-components-storybook-starter",
  "description": "A bare-bones Tailwind CSS + Styled-Components + Storybook starter for Gatsby",
  "version": "1.0.0",
  "author": "Dennis Vash <dennisvash@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "template",
    "tailwind",
    "storybook",
    "styled-components",
    "css-in-js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/denvash/gatsby-styled-tailwind-storybook-starter"
  },
  "bugs": {
    "url": "https://github.com/denvash/gatsby-styled-tailwind-storybook-starter/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -o",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "storybook": "NODE_ENV=production start-storybook -p 6006",
    "build-storybook": "yarn build; NODE_PATH=src/ NODE_ENV=production build-storybook -s public --quiet"
  },
  "dependencies": {
    "core-js": "2",
    "deck.gl": "^7.3.6",
    "gatsby": "^2.17.13",
    "lodash.debounce": "^4.0.8",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^3.28.1",
    "react-map-gl": "^5.1.2",
    "react-range": "^1.4.3",
    "styled-components": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@dennisvash/eslint-config": "1.1.0",
    "@dennisvash/prettier-config": "^1.0.0",
    "@storybook/react": "^5.2.6",
    "babel-eslint": "^10.0.3",
    "babel-plugin-remove-graphql-queries": "^2.7.15",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.21",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-manifest": "^2.2.27",
    "gatsby-plugin-now": "^1.2.5",
    "gatsby-plugin-offline": "^3.0.20",
    "gatsby-plugin-postcss": "^2.1.14",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-styled-components": "^3.1.12",
    "gatsby-source-graphql": "^2.1.22",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.1.3"
  }
}
