{
  "name": "gatsby-tailwind-styled-components-storybook-starter",
  "description": "A bare-bones Tailwind CSS + Styled-Components + Storybook starter for Gatsby",
  "version": "1.0.0",
  "author": "Dennis Vash <dennisvash@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "template",
    "tailwind",
    "storybook",
    "styled-components",
    "css-in-js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/denvash/gatsby-styled-tailwind-storybook-starter"
  },
  "bugs": {
    "url": "https://github.com/denvash/gatsby-styled-tailwind-storybook-starter/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -o",
    "format": "prettier --write src/**/*",
    "lint": "eslint src/**/*.js",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "storybook": "NODE_ENV=production start-storybook -p 6006",
    "build-storybook": "yarn build; NODE_PATH=src/ NODE_ENV=production build-storybook -s public --quiet"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.7",
    "axios": "^0.19.0",
    "core-js": "2",
    "deck.gl": "^8.0.1",
    "gatsby": "^2.18.17",
    "gatsby-source-graphql": "^2.1.29",
    "graphql": "^14.5.8",
    "isomorphic-fetch": "^2.2.1",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.24.0",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "rc-slider": "^8.7.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-graph-vis": "^1.0.5",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^4.4.1",
    "react-map-gl": "^5.1.6",
    "reusable": "^1.0.0-alpha.12",
    "styled-components": "^5.0.0-rc.2",
    "use-http": "^0.2.4"
  },
  "devDependencies": {
    "@dennisvash/eslint-config": "^2.2.2",
    "@dennisvash/prettier-config": "^1.0.0",
    "@storybook/react": "^5.2.8",
    "babel-eslint": "^10.0.3",
    "babel-plugin-remove-graphql-queries": "^2.7.19",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.26",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-offline": "^3.0.30",
    "gatsby-plugin-postcss": "^2.1.18",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-styled-components": "^3.1.16",
    "graphql-tag": "^2.10.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.1.4"
  }
}
